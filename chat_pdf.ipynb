        "from pypdf import PdfReader\n",
        "def get_pdf_text(pdf):\n",
        "  text = \"\"\n",
        "  pdf_reader = PdfReader(pdf)\n",
        "  for page in pdf_reader.pages:\n",
        "    text += page.extract_text()\n",
        "  return text\n"
      ]
